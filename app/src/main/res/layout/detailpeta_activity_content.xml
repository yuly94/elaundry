    <include layout="@layout/map_toolbar"/>

    <include layout="@layout/peta_navigasi_detail"/>

    <include layout="@layout/app_settings"/>

    <include layout="@layout/map_nav_settings"/>

    <include layout="@layout/map_nav_settings_dari"/>

    <include layout="@layout/map_nav_settings_tujuan"/>

    <include layout="@layout/map_nav_instruction"/>

